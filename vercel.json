{
  "version": 2,
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.inch.qa" }],
      "destination": "https://inch.qa/$1",
      "permanent": true
    },
    { "source": "/index.html", "destination": "/", "permanent": true }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
